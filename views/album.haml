- @meta_title = "fewsel | photos and videos"
- @meta_description = "Photos and Videos of fewsel"

%h1.red= "fewsel in color"

%ul#images{ :data => { :collection => "photos" } }
  - album.photos.each do |photo|
    %li
      %a.img_wrap{ :href => photo.url, :rel => "photo", :title => photo.name }
        %div.cover{ :style => "background-image: url(#{photo.thumb})" }


